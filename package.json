{
  "name": "react-slide",
  "version": "0.2.0",
  "description": "React Slider",
  "main": "lib/index.js",
  "scripts": {
    "test": "make",
    "lib": "./node_modules/.bin/gulp",
    "build-style": "./node_modules/.bin/webpack --progress --colors --config build-style.config.js",
    "dist": "./node_modules/.bin/webpack --progress --colors --config dist.config.js",
    "dist.min": "./node_modules/.bin/webpack --progress --colors --optimize-minimize --optimize-occurence-order --optimize-dedupe --config dist.min.config.js",
    "build": "npm run lib && npm run dist && npm run dist.min",
    "serve": "./node_modules/.bin/http-server -p 9091",
    "dev": "./node_modules/.bin/webpack-dev-server --progress --colors --port 9090 --content-base http://localhost:9091"
  },
  "devDependencies": {
    "css-loader": "^0.9.0",
    "extract-text-webpack-plugin": "^0.3.5",
    "gulp": "^3.8.11",
    "gulp-react": "^3.0.0",
    "http-server": "^0.7.4",
    "jsx-loader": "^0.12.2",
    "nib": "^1.0.4",
    "style-loader": "^0.8.2",
    "stylus": "^0.49.3",
    "stylus-loader": "^0.5.0",
    "webpack": "^1.4.13",
    "webpack-dev-server": "^1.6.6"
  },
  "dependencies": {
    "drag-helper": "^1.2.3",
    "object-assign": "^2.0.0",
    "react-event-names": "^1.0.0",
    "react-style-normalizer": "^1.0.2",
    "region": "^2.1.2"
  },
  "peerDependencies": {
    "react": ">=0.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/radubrehar/react-slide.git"
  },
  "keywords": [
    "slider",
    "slide",
    "slider",
    "field",
    "react-component",
    "react"
  ],
  "author": "Radu Brehar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/radubrehar/react-slide/issues"
  },
  "homepage": "https://github.com/radubrehar/react-slide"
}
